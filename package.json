    "ramda": "^0.27.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-spread": "^7.7.4",
    "auto-reload-brunch": "^2.7.1",
    "babel-brunch": "7.0.1",
    "brunch": "3.0.0",
    "clean-css-brunch": "3.0.0",
    "copycat-brunch": "1.1.1",
